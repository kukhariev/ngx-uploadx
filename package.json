{
  "name": "ngx-uploadx",
  "version": "3.4.0",
  "description": "Angular Resumable Upload Module",
  "keywords": [
    "angular",
    "upload",
    "chunk",
    "resumable",
    "tus"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/ngx-uploadx.git"
  },
  "homepage": "https://github.com/kukhariev/ngx-uploadx#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test --watch=false --progress=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "server": "node ./server/ --debug",
    "server:dev": "node ./server/ --errors --debug",
    "build": "ts-node --skip-project ./scripts/build.ts",
    "ci": "npm run lint && npm run test && npm run e2e",
    "prettier": "prettier --write \"**/*.{js,json,scss,md,ts,html}\"",
    "test:integration:v10": "cd integrations/v10 && npm run test",
    "serve:static:integration:v10": "angular-http-server --path integrations/v10/dist/v10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.10",
    "@angular/cli": "7.3.10",
    "@angular/common": "7.2.16",
    "@angular/compiler": "7.2.16",
    "@angular/compiler-cli": "7.2.16",
    "@angular/core": "7.2.16",
    "@angular/platform-browser": "7.2.16",
    "@angular/platform-browser-dynamic": "7.2.16",
    "@angular/router": "7.2.16",
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@commitlint/travis-cli": "9.0.1",
    "@types/jasmine": "3.5.11",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "10.17.26",
    "angular-http-server": "^1.9.0",
    "codelyzer": "5.2.2",
    "core-js": "2.6.11",
    "husky": "4.2.5",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "10.2.11",
    "ng-packagr": "5.7.1",
    "node-uploadx": "4.1.3",
    "prettier": "2.0.5",
    "protractor": "7.0.0",
    "rimraf": "^3.0.1",
    "rxjs": "6.5.5",
    "ts-node": "8.10.2",
    "tsickle": "0.39.1",
    "tslib": "~1.13.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.3.0",
    "typescript": "3.2.4",
    "zone.js": "0.10.3"
  },
  "peerDependencies": {
    "@angular/core": ">=6.0.0 <11.0.0",
    "rxjs": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,scss,md,ts,html}": [
      "prettier --write"
    ]
  }
}

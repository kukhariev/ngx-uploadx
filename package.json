{
  "name": "ngx-uploadx",
  "version": "3.2.0",
  "description": "Angular Resumable Upload Module",
  "keywords": [
    "angular",
    "upload",
    "chunk",
    "tus"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/ngx-uploadx.git"
  },
  "homepage": "https://github.com/kukhariev/ngx-uploadx#readme",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier": "prettier --write \"**/*.{json,scss,md,ts,html}\"",
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test --watch=false --progress=false --browsers=ChromeHeadless",
    "e2e": "ng e2e -c=ci --webdriver-update=false",
    "server": "node ./server/ ",
    "server:dev": "node ./server/ --errors --log",
    "build": "ts-node --skip-project ./scripts/build.ts",
    "ci": "npm run lint && npm run test && npm run webdriver-update-ci && npm run e2e",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 76.0.3809.68"

  },
  "dependencies": {
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.6.3",
    "rxjs": "~6.5.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@commitlint/travis-cli": "^8.0.0",
    "@schuchard/prettier": "^3.1.0",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "codelyzer": "^5.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.4.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "8.2.0",
    "ng-packagr": "^5.3.0",
    "node-uploadx": "^3.0.0",
    "prettier": "1.17.1",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.1",
    "tsickle": "0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.2.4"
  },
  "peerDependencies": {
    "@angular/core": ">=6.0.0 <9.0.0",
    "rxjs": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,scss,md,ts,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}

{
  "name": "ngx-uploadx",
  "version": "4.1.4",
  "description": "Angular Resumable Upload Module",
  "keywords": [
    "angular",
    "chunk",
    "resumable",
    "tus",
    "upload",
    "uploadx"
  ],
  "author": {
    "name": "Oleg Kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/ngx-uploadx.git"
  },
  "homepage": "https://github.com/kukhariev/ngx-uploadx#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.12.0",
    "npm": "<7.0.0"
  },
  "scripts": {
    "start": "run-p serve:dev server",
    "test": "ng test --watch=false --progress=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "serve:dev": "ng serve",
    "server": "node ./server/ --debug",
    "build:pkg": "node scripts/build.js",
    "build:ng": "node scripts/build-app",
    "build:prod": "ng build --configuration production",
    "serve:prod": "angular-http-server --path dist/app",
    "build:latest": "npm run build:ng latest",
    "serve:latest": "angular-http-server --path integrations/latest/dist/latest",
    "prepare": "husky install",
    "browserstack": "ng e2e --configuration browserstack --port 9091",
    "prettier": "prettier --write \"**/*.{js,json,scss,md,ts,html}\""
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "~12.2.13",
    "@angular/common": "~12.2.13",
    "@angular/compiler": "~12.2.13",
    "@angular/compiler-cli": "~12.2.13",
    "@angular/core": "~12.2.13",
    "@angular/platform-browser": "~12.2.13",
    "@angular/platform-browser-dynamic": "~12.2.13",
    "@angular/router": "~12.2.13",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/cpx": "^1.5.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "12.12.41",
    "@uploadx/core": "^4.5.0",
    "angular-http-server": "^1.10.0",
    "browserstack-local": "^1.4.8",
    "codelyzer": "^6.0.0",
    "core-js": "2.6.11",
    "cpx": "^1.5.0",
    "husky": "^7.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.1.2",
    "ng-packagr": "^12.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "~6.6.7",
    "ts-node": "^10.2.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.3.5",
    "zone.js": "~0.11.4"
  },
  "peerDependencies": {
    "@angular/core": ">=12.0.0 <14.0.0",
    "rxjs": "^6.6.0"
  },
  "lint-staged": {
    "*.{js,json,scss,md,ts,html}": [
      "prettier --write"
    ]
  }
}

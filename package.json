{
  "name": "ngx-uploadx",
  "version": "3.3.2",
  "description": "Angular Resumable Upload Module",
  "keywords": [
    "angular",
    "upload",
    "chunk",
    "resumable",
    "tus"
  ],
  "author": {
    "name": "kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/ngx-uploadx.git"
  },
  "homepage": "https://github.com/kukhariev/ngx-uploadx#readme",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier": "prettier --write \"**/*.{json,scss,md,ts,html}\"",
    "start": "ng serve",
    "lint": "ng lint",
    "test": "ng test --watch=false --progress=false --browsers=ChromeHeadless",
    "e2e": "ng e2e -c=ci --webdriver-update=false",
    "server": "node ./server/ --debug",
    "server:dev": "node ./server/ --errors --debug",
    "build": "ts-node --skip-project ./scripts/build.ts",
    "ci": "npm run lint && npm run test && npm run webdriver-update-ci && npm run e2e",
    "webdriver-update-ci": "webdriver-manager update --standalone false --gecko false --versions.chrome 78.0.3904.105"
  },
  "dependencies": {
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/forms": "7.2.15",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/router": "7.2.15",
    "core-js": "2.6.11",
    "rxjs": "6.5.3",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.9",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@angular/language-service": "7.2.15",
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/travis-cli": "8.3.4",
    "@types/jasmine": "3.5.0",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "10.17.13",
    "codelyzer": "5.2.1",
    "husky": "3.1.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "lint-staged": "9.5.0",
    "ng-packagr": "5.7.1",
    "node-uploadx": "4.0.0",
    "prettier": "1.19.1",
    "protractor": "5.4.2",
    "ts-node": "8.5.4",
    "tsickle": "0.34.0",
    "tslib": "1.10.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.2.4"
  },
  "peerDependencies": {
    "@angular/core": ">=6.0.0 <10.0.0",
    "rxjs": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,scss,md,ts,html}": [
      "prettier --write",
      "git add"
    ]
  }
}

{
  "name": "ngx-uploadx",
  "version": "5.1.0",
  "description": "Angular Resumable Upload Module",
  "keywords": [
    "angular",
    "chunk",
    "resumable",
    "tus",
    "upload",
    "uploadx"
  ],
  "author": {
    "name": "Oleg Kukhariev",
    "email": "q_h@inbox.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kukhariev/ngx-uploadx.git"
  },
  "homepage": "https://github.com/kukhariev/ngx-uploadx#readme",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.15.0",
    "npm": "^6.0.0 || ^8.16.0"
  },
  "scripts": {
    "start": "run-p serve:dev server",
    "test": "ng test",
    "coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "serve:dev": "ng serve",
    "server": "node ./server/",
    "server:debug": "node ./server/ --debug",
    "build:pkg": "node scripts/build.js",
    "build:ng": "node scripts/build-app",
    "build:prod": "ng build --configuration production",
    "serve:prod": "angular-http-server --path dist/app",
    "build:latest": "npm run build:ng latest",
    "serve:latest": "angular-http-server --path integrations/latest/dist/latest",
    "playwright": "npx playwright test -c e2e/playwright.config.ts",
    "e2e": "npx playwright test -c e2e/e2e.config.ts",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{js,json,scss,md,ts,html}\""
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular-eslint/builder": "12.7.0",
    "@angular-eslint/eslint-plugin": "12.7.0",
    "@angular-eslint/eslint-plugin-template": "12.7.0",
    "@angular-eslint/schematics": "12.7.0",
    "@angular-eslint/template-parser": "12.7.0",
    "@angular/cli": "~12.2.17",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@playwright/test": "^1.25.0",
    "@types/cpx": "^1.5.2",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^14.18.20",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "@uploadx/core": "^5.1.4",
    "angular-http-server": "^1.10.0",
    "cpx": "^1.5.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.0.0",
    "ng-packagr": "^12.2.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.2",
    "rimraf": "^3.0.2",
    "rxjs": "~6.6.7",
    "ts-node": "^10.2.1",
    "tslib": "^2.0.0",
    "typescript": "~4.3.5",
    "zone.js": "~0.11.4"
  },
  "peerDependencies": {
    "@angular/core": ">=12.0.0 <15.0.0",
    "rxjs": "^6.4.0 || ^7.0.0"
  },
  "lint-staged": {
    "*.{js,json,scss,md,ts,html}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
